{
  "@context": "https://schema.org/",
  "@type": "Review",
  "itemReviewed": {
    "@type": "Product",
    "name": "{{productName}}",
    "image": "{{productImage}}",
    "description": "{{productDescription}}",
    "brand": {
      "@type": "Brand",
      "name": "{{brandName}}"
    }
  },
  "reviewRating": {
    "@type": "Rating",
    "ratingValue": "{{rating}}",
    "bestRating": "5",
    "worstRating": "1"
  },
  "author": {
    "@type": "Person",
    "name": "{{reviewerName}}"
  },
  "reviewBody": "{{reviewText}}",
  "datePublished": "{{publishDate}}",
  "publisher": {
    "@type": "Organization",
    "name": "MakeupHub",
    "sameAs": "https://makeupdir.com"
  }
}
