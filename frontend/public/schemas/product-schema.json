{
  "@context": "https://schema.org/",
  "@type": "Product",
  "name": "{{productName}}",
  "image": "{{productImage}}",
  "description": "{{productDescription}}",
  "brand": {
    "@type": "Brand",
    "name": "{{brandName}}"
  },
  "offers": {
    "@type": "Offer",
    "url": "{{productUrl}}",
    "priceCurrency": "USD",
    "price": "{{price}}",
    "availability": "https://schema.org/InStock",
    "seller": {
      "@type": "Organization",
      "name": "MakeupHub"
    }
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "{{rating}}",
    "reviewCount": "{{reviewCount}}"
  },
  "review": {
    "@type": "Review",
    "reviewRating": {
      "@type": "Rating",
      "ratingValue": "{{rating}}",
      "bestRating": "5"
    },
    "author": {
      "@type": "Person",
      "name": "{{reviewerName}}"
    }
  }
}
